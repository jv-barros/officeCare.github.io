{
  "name": "officecare-server",
  "version": "0.1.0",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "migrate": "npx sequelize db:migrate",
    "seed": "node ./seeders/seed-admin.js",
    "seed:demo": "node ./seeders/seed-demo-data.js",
    "fix-db-auth": "docker-compose exec db mariadb -u root -prootpassword -e \"DROP USER IF EXISTS 'officecare'@'%'; CREATE USER 'officecare'@'%' IDENTIFIED BY 'officecare'; GRANT ALL PRIVILEGES ON officecare_dev.* TO 'officecare'@'%'; FLUSH PRIVILEGES;\"",
    "serve:frontend": "npx http-server .. -p 8000 -c-1"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-mysql-session": "^2.1.7",
    "express-rate-limit": "^6.7.0",
    "express-session": "^1.17.3",
    "express-validator": "^6.15.0",
    "helmet": "^7.0.0",
    "mysql2": "^3.16.0",
    "sequelize": "^6.32.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  }
}
